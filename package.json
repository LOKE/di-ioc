{
  "name": "di-ioc",
  "version": "1.0.0",
  "description": "Node.js dependency injection.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "consistent-return": 0,
      "no-return-assign": 0,
      "key-spacing": 0,
      "no-multi-spaces": 0,
      "no-use-before-define": [2, "nofunc"],
      "curly": [2, "multi-line"],
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aantthony/di-ioc.git"
  },
  "author": "Anthony Foster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aantthony/di-ioc/issues"
  },
  "homepage": "https://github.com/aantthony/di-ioc#readme",
  "dependencies": {
    "fn-annotate": "^1.0.1",
    "private": "https://github.com/aantthony/private/archive/v1.1.0.tar.gz"
  },
  "devDependencies": {
    "eslint": "^0.24.0"
  }
}
